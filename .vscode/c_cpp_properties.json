            "defines": [
                "FREERTOS",
                "ESP32_IDF",
                "STEPPER_SERVO",
                "HOSTNAME=drive",
                "COMMAND",
                "MQTT_PORT=1883",
                "MAIN_BATTERY"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "clang-x64",
            "forcedInclude": [],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
